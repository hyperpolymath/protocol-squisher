// Test pattern matching

fn classify(x: i32) -> i32 {
    match x {
        0 => 100,
        1 => 200,
        2 => 300,
        _ => 999
    }
}

fn is_zero(x: i32) -> bool {
    match x {
        0 => true,
        _ => false
    }
}

fn main() {
    let a = classify(0);
    let b = classify(1);
    let c = classify(5);
    a + b + c
}
