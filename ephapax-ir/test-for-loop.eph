// Test for loop over vector
// Since we don't have mutable variables yet, this just demonstrates iteration
// The for loop returns the result of the last iteration

// Create a vector of numbers
fn make_numbers() -> Vec<i32> {
    [10, 20, 30, 40, 50]
}

// Iterate over vector and return last element doubled
fn process_vec(vec: Vec<i32>) -> i32 {
    for elem in vec {
        let doubled = elem + elem;
        doubled
    }
}

// Main: process [10, 20, 30, 40, 50]
// Last element is 50, so result is 50 + 50 = 100
fn main() {
    let numbers = make_numbers();
    process_vec(numbers)
}
