// Test bitwise operators (&, |, ^, <<, >>)

fn test_bitwise_and() -> i32 {
    15 & 7  // 1111 & 0111 = 0111 = 7
}

fn test_bitwise_or() -> i32 {
    8 | 4  // 1000 | 0100 = 1100 = 12
}

fn test_bitwise_xor() -> i32 {
    15 ^ 7  // 1111 ^ 0111 = 1000 = 8
}

fn test_shift_left() -> i32 {
    1 << 3  // 0001 << 3 = 1000 = 8
}

fn test_shift_right() -> i32 {
    16 >> 2  // 10000 >> 2 = 100 = 4
}

fn main() {
    let a = test_bitwise_and();
    let b = test_bitwise_or();
    let c = test_bitwise_xor();
    let d = test_shift_left();
    let e = test_shift_right();
    a + b + c + d + e  // 7 + 12 + 8 + 8 + 4 = 39
}
