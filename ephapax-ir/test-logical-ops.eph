// Test logical operators (&&, ||)

fn test_and() -> bool {
    true && true
}

fn test_or() -> bool {
    false || true
}

fn test_combined() -> i32 {
    if true && false {
        100
    } else {
        if false || true {
            200
        } else {
            300
        }
    }
}

fn main() {
    let a = test_and();
    let b = test_or();
    let c = test_combined();
    if a && b {
        c
    } else {
        0
    }
}
