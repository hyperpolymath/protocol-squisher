// Test struct support

// Define a Point struct
struct Point {
    x: i32,
    y: i32,
}

// Define a Person struct with multiple field types
struct Person {
    name: String,
    age: i32,
    active: bool,
}

// Create a point
fn make_point(x_val: i32, y_val: i32) -> Point {
    Point { x: x_val, y: y_val }
}

// Access a field
fn get_x(p: Point) -> i32 {
    p.x
}

// Main entry point
fn main() {
    let point = make_point(10, 20);
    get_x(point)  // Should return 10
}
