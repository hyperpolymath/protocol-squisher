// Test HashMap built-in functions
fn main() -> i32 {
    // Create empty HashMap
    let map = hashmap_new();

    // Insert key-value pairs
    let map = hashmap_insert(map, "name", "Alice");
    let map = hashmap_insert(map, "role", "Engineer");
    let map = hashmap_insert(map, "level", "Senior");

    // Test remove operation first (before borrowing)
    let map = hashmap_remove(map, "level");

    // Test get operation
    let result = match hashmap_get(&map, "name") {
        Some(val) => {
            print(val);
            0
        }
        None => 1
    };

    // Test contains_key
    let has_role = hashmap_contains_key(&map, "role");
    let has_invalid = hashmap_contains_key(&map, "invalid");
    let still_has_level = hashmap_contains_key(&map, "level");

    if has_role && !has_invalid && !still_has_level {
        0
    } else {
        1
    }
}
