// Comprehensive Vec<T> test demonstrating various features

// Test 1: Empty vector (type defaulting)
fn empty_vec() -> Vec<i32> {
    []  // Empty vec defaults to Vec<i32>
}

// Test 2: Homogeneous vector
fn int_vec() -> Vec<i32> {
    [1, 2, 3, 4, 5]
}

// Test 3: Nested function calls with vectors
fn nested_calls() -> i32 {
    let v = [100, 200, 300];
    v[0]
}

// Test 4: Vector as parameter
fn first_element(vec: Vec<i32>) -> i32 {
    vec[0]
}

// Test 5: Multiple vector types (different element types)
fn bool_vec() -> Vec<bool> {
    [true, false, true]
}

fn string_vec() -> Vec<String> {
    ["hello", "world"]
}

// Main test runner
fn main() {
    nested_calls()  // Should return 100
}
